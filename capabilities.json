            "text": true
          }
        },
        "maximumrangeX": {
          "type": {
            "text": true
          }
        }
      }
    },
    "rangeY": {
      "displayName": "Range Y-Axis",
      "properties": {
        "minimumrangeY": {
          "type": {
            "text": true
          }
        },
        "maximumrangeY": {
          "type": {
            "text": true
          }
        }
      }
    }
