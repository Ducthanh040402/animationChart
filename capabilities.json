      "displayName": "Data Point Settings",
      "properties": {
        "defaultColor": {
          "displayName": "Chart Color",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "colorAfterTransition": {
          "displayName": "Color Transition",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "fontSize": {
          "displayName": "Point Size",
          "type": {
            "numeric": true
          }
        },
        "sortData": {
          "type": {
            "bool": true
          }
        }
      }
    },
    "transitionSetting": {
      "displayName": "Transition (Default 1s)",
      "properties": {
        "speedTransition": {
          "type": {
            "numeric": true
          }
        },
        "lineorPoint": {
          "type": {
            "bool": true
          }
        },
        "showPoint": {
          "type": {
            "bool": true
          }
        }
      }
    },
    "ishowGrid": {
      "displayName": "Axis and Gridlines",
      "properties": {
        "showGird": {
          "type": {
            "bool": true
          }
        },
        "showAxis": {
          "type": {
            "bool": true
          }
