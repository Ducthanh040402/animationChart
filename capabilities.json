  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "for": {
            "in": "x-value"
          },
          "dataReductionAlgorithm": {
            "window": {
              "count": 30000
            }
          }
        },
        "values": {
          "select": [
            {
              "for": {
                "in": "y-value"
              },
              "dataReductionAlgorithm": {
                "window": {
                  "count": 30000
                }
              }
            },
            {
              "for": {
                "in": "index"
              },
              "dataReductionAlgorithm": {
                "window": {
                  "count": 30000
                }
              }
            }
          ]
        }
      }
    }
